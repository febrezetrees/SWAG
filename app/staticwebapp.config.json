{
  "platform": {
    "apiRuntime": "node:16"
  },
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "routes": [
    {
    "route": "/catalog",
    "redirect": "/products",
    "statusCode": 301
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": 404
    },
    {
      "route": "/api/products*",
      "allowedRoles": ["authenticated"]
    }
  ]
}